  "compilerOptions": {
    "module": "esnext",
    "target": "esnext",
    "noEmit": true,
    "types": ["svelte", "@types/snowpack-env"],
    "strict": true,
    "resolveJsonModule": true,
    "useDefineForClassFields": true
  }
}
